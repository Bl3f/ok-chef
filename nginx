docker run
    -it
    -p 80:80
    -p 443:443

    -v /opt/nginx/conf.d:/etc/nginx/conf.d:ro

    -v /opt/fshare/static:/statics/fshare
    -v /opt/polaroid/static:/statics/polaroid
    -v /opt/freespeech/static:/statics/fsp

    -v /etc/letsencrypt:/certs/

    -v /opt/fshare/fshare/fshare.sock:/sockets/fshare.sock
    -v /opt/polaroid/polaroid.sock:/sockets/polaroid.sock
    -v /opt/freespeech/fsp.sock:/sockets/fsp.sock

    --name nginx
    -d nginx